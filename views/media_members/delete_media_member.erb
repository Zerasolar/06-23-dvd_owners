<h1> Deleting a Media Member</h1>

<form action="/gone_media_member">

  Which one do you want to delete?<br>
  <% MediaMember.all.each do |list| %>
  <% @member = Member.find(list.member_id)%>
  <% @media = Media.find(list.media_id)%>
  <input type="checkbox" name= "mediamember[]" value= "<%= list.id%>"> <%= @member.name%> <%= @media.cover_name%><br>
  <% end %>
  
  <input type= "submit">
</form>

<a href="/media_menu">Back</a>